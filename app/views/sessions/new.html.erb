<div class="container">
<div class="title">
Tweets
</div>
<div class="sub-title">
--delburger
</div>
<%= form_for(:session, url: login_path) do |f| %>

      <div class="lab">
      <%= f.label :name %>
      </div>
      <div class="text-field">
      <%= f.text_field :name, class: 'form-control' %>
      </div>
      </br>
      <div class="lab"> 
      <%= f.label :password %>
      </div>
      <div class="text-field">
      <%= f.password_field :password, class: 'form-control' %>
      </div>
      </br>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
<a href="/users/new" >sign up</a>
</div>
